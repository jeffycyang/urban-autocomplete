{
  "manifest_version": 2,
  "name": "Urban Autocomplete",
  "version": "0.1",
  "content_scripts": [
    {
	  "matches": ["<all_urls>"],
      "css": ["awesomplete.css"],
      "js": ["jquery-2.1.4.min.js", "awesomplete.js", "autocomplete.js"]
    }
  ],
  "background": {
  	"scripts": ["background.js"],
  	"persistent": false
  }
}